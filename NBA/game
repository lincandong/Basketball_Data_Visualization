			var stat = new Array();
			var team_stat = new Array();
			var last_pbp_class = 'all';
			var away_change_all_html = $('#stat_boxCLE').html();
			var home_change_all_html = $('#stat_boxTOR').html();
			function change(starttime, endtime, pbp_class){
				var start_i_index = 0;
				var end_i_index = player_id.length - 1;
				while(time[start_i_index] <= starttime && start_i_index <= end_i_index)
				{
					start_i_index++;
				}
				while(time[end_i_index] > endtime && start_i_index <= end_i_index)
					end_i_index--;
				if(currentcol[0] != 'gs' || colorder[0] != 1)
				{
					colorder[0] = 1;
					to_current(0,'gs','stat_boxCLE');
				}
				if(currentcol[1] != 'gs' || colorder[1] != 1)
				{
					colorder[1] = 1;
					to_current(1,'gs','stat_boxTOR');
				}
				
				if(pbp_class == 'all')
				{
					$('#stat_boxCLE').html(away_change_all_html);
					$('#stat_boxTOR').html(home_change_all_html);
				}
				else{
					for(var bi = 0; bi <= 1; bi++){
						var html = '';
						var k_length;
						if(bi == 0)
							k_length = away_player.length;
						else
							k_length = home_player.length;
						for(var i = 0; i <= col.length; i++)
							team_stat[i] = 0;
						for(var k = 0; k < k_length; k++){
							if(bi == 0)
							{
								gs = away_gs[k];
								player_name_out = away_player_name_out[k];
								current_player_id = away_player[k];
							}
							else
							{
								gs = home_gs[k];
								player_name_out = home_player_name_out[k];
								current_player_id = home_player[k];
							}
							for(var i = 0; i <= col.length; i++)
								stat[i] = 0;
							var doth = 0;
							for(var i = start_i_index; i <= end_i_index; i++){
								if(player_id[i] == current_player_id)
								{
									if(item[i] == '2p'){
										doth = 1;
										stat[4]++;
									}
									if(item[i] == 'm2p'){
										doth = 1;
										stat[5]++;
									}
									if(item[i] == '3p'){
										doth = 1;
										stat[7]++;
									}
									if(item[i] == 'm3p'){
										doth = 1;
										stat[8]++;
									}
									if(item[i] == 'ft'){
										doth = 1;
										stat[10]++;
									}
									if(item[i] == 'mft'){
										doth = 1;
										stat[11]++;
									}
									if(item[i] == 'orb'){
										doth = 1;
										stat[13]++
									}
									if(item[i] == 'drb'){
										doth = 1;
										stat[14]++
									}
									if(item[i] == 'ast'){
										doth = 1;
										stat[15]++
									}
									if(item[i] == 'stl'){
										doth = 1;
										stat[16]++
									}
									if(item[i] == 'blk'){
										doth = 1;
										stat[17]++
									}
									if(item[i] == 'tov'){
										doth = 1;
										stat[18]++
									}
									if(item[i] == 'pf'){
										doth = 1;
										stat[19]++
									}
								}
							}
							for(var i = 0; i <= col.length; i++)
								team_stat[i] += stat[i];
							if(doth != 2){
								fill_stat(stat);
								html += '<tr class="sort">';
								html += '<td style="border:0px;"></td>';
								html += '<td class ="normal player_name_out change_color col0 row'+k+'"><a href="/player/'+current_player_id+'.html" target="_blank">' + player_name_out +'</a></td>';
								html += '<td class ="current gs change_color col1 row'+k+'" rank="'+gs+'">'+'-'+'</td>';
								for(var i = 2; i < col.length; i++){
									if(stat[i] == -1)
										v = ''
									else
										v = stat[i];
									html += '<td class ="normal ' + col[i] +' change_color col'+i+' row'+k+'" rank="'+stat[i]+'">' + v+'</td>';
								}
								html += '</tr>';
							}
						}
						if(bi == 0)
							box = 'stat_boxCLE';
						else
							box = 'stat_boxTOR';
						$("#" + box+" .sort").remove();
						$("#" + box+" tr:eq("+(1) + ")").before(html);
					
						html = '';
						fill_stat(team_stat);	
						html += '<tr class="team_all_content">';
						html += '<td colspan="2" class="normal" >-</td>';
						html += '<td colspan="2" class ="normal">-</td>';
						for(var i = 3; i < col.length; i++){
							if(team_stat[i] == -1)
								v = ''
							else
								v = team_stat[i];
							html += '<td class ="normal '+col[i]+'">' + v+'</td>';
						}
						html += '</tr>';
						$("#" + box+" .team_all_content").remove();
						$("#" + box+" .team_all_tr:eq("+(0) + ")").after(html);
					}
					
				}
				
				detail(starttime, endtime, pbp_class);
				
				change_color();
				$(".pbp_" + pbp_class).attr('class',$(".pbp_" + pbp_class).attr('class').replace('pbp_chooser','pbp_chooser_in'));
				$(".pbp_" + last_pbp_class).attr('class',$(".pbp_" + last_pbp_class).attr('class').replace('pbp_chooser_in','pbp_chooser'));
				last_pbp_class = pbp_class;
			}
			function fill_stat(stat){
				stat[2] = -1;
				stat[4] += stat[7];
				stat[5] += stat[8];
				for(dx = 3; dx <= 9; dx += 3)
				{
					stat[dx + 2] += stat[dx+1];
					if(stat[dx + 2] == 0)
						stat[dx] = '-1';
					else
					{
						var stat_t = stat[dx + 1] * 100.0 / stat[dx + 2];
						stat[dx] = Math.floor(stat_t) + '.' + (Math.round(stat_t * 10) % 10) + '%';
					}
				}
				stat[2] = '-';
				stat[20] = stat[4] * 2 + stat[7] + stat[10];
				stat[12] = stat[13] + stat[14];
				for(var i = 21; i >= 13; i--)
					stat[i] = stat[i-1];
				stat[12] = -1;
			}
			
			function detail(starttime, endtime, pbp_class){
				var html = '';
				if(pbp_class == 'all' || pbp_class=='1h' || pbp_class == '2h') {
					html += '<tr>';
					html += '<td>请选择上面按钮中的时间段</td>';
					html += '<td>克里夫兰骑士</td>';
					html += '<td>-</td>';
					html += '<td style="font-weight:bold">113-112</td>';
					html += '<td>-</td>';
					html += '<td>多伦多猛龙</td>';
					html += '</tr>';
					$('#detail_q').html(html);
					return;
				}
				var score = new Array(0,0);
				for(var i = 0; i < player_id.length;i++)
				{
					if(time[i] <= endtime)
					{
						var ct = endtime - time[i];
						var m = Math.floor(ct / 60 / 10);
						var s = Math.floor((ct % 600) / 10);
						if(m < 10)
							m = '0' + m;
						if(s < 10)
							s = '0' + s;
						
						var event = new Array('','');
						var il = new Array('','');
						var first = 1;
						while(i < player_id.length && (time[i - 1] == time[i] || first == 1)){
							var name;
							var length;
							var bi;
							var word = '';
							var ii = '';
							if(team[i] == 'CLE'){
								bi = 0;
								length = away_player.length;
							}
							else{
								bi = 1;
								length = home_player.length;
							}
							for(var t = 0; t < length; t++)
							{
								if(bi == 0){
									if(player_id[i] == away_player[t])
										name = away_player_name_out[t];
								}
								else{
									if(player_id[i] == home_player[t])
										name = home_player_name_out[t];
								}
							}
							name = '<a target="_blank" href="/player/'+player_id[i]+'.html">' + name + '</a>';
							if(item[i] == 'm2p') {
								word = name + '两分打铁';
								ii = '打铁+1';
							}
							if(item[i] == 'm3p') {
								word = name + '三分打铁';
								ii = '打铁+1';
							}
							if(item[i] == 'mft') {
								word = name + '罚球打铁';
								ii = '打铁+1';
							}
							if(item[i] == '2p') {
								word = name + '命中两分';
								ii = '得分+2';
								score[bi] += 2;
							}
							if(item[i] == '3p'){
								word = name + '命中三分';
								ii = '得分+3';
								score[bi] += 3;
							}
							if(item[i] == 'ft') {
								word = name + '命中罚球';
								ii = '得分+1';
								score[bi] += 1;
							}
							if(item[i] == 'orb') {
								word = name + '抢到进攻篮板';
								ii = '篮板+1';
							}
							if(item[i] == 'drb') {
								word = name + '抢到防守篮板';
								ii = '篮板+1';
							}
							if(item[i] == 'ast') {
								word = name + '助攻队友';
								ii = '助攻+1';
							}
							if(item[i] == 'blk') {
								word = name + '盖帽对方';
								ii = '盖帽+1';
							}
							if(item[i] == 'stl') {
								word = name + '抢断对手';
								ii = '抢断+1';
							}
							if(item[i] == 'tov') {
								word = name + '失误';
								ii = '失误+1';
							}
							if(item[i] == 'in') {
								word = name + '上场';
							}
							if(item[i] == 'out') {
								word = name + '被换下';
							}
							if(item[i] == 'tf') {
								word = name + '技术犯规';
								ii = '技规+1'
							}
							if(item[i] == 'pf') {
								word = name + '犯规';
								ii = '犯规+1'
							}
							event[bi] += word + '<br/>';
							il[bi] += ii + '<br/>';
							first = 0;
							i++;
						}
						i--;
						if(starttime < time[i])
						{
							html += '<tr>';
							html += '<td>' + m + ':' + s + '.' +(ct % 10)+ '</td>';
							html += '<td>' + event[0] + '</td>';
							html += '<td>' + il[0] + '</td>';
							html += '<td style="font-weight:bold">' + score[0] + '-'+ score[1] + '</td>';
							html += '<td>' + il[1] + '</td>';
							html += '<td>' + event[1] + '</td>';
							html += '</tr>';
						}
					}
				}
				$('#detail_q').html(html);
			}
			detail(-1,-1,'all');
			
		</script>
<script type="text/javascript" id='speedup_test' src="https://idm-su.baidu.com/su.js"></script>
</body>
</html>
