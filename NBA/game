					$("#" + box+" tr:eq(0) ."+ 'current' +"").attr("class" , $("#" + box+" tr:eq(0) ."+ 'current' +"").attr("class").replace('current','normal'));
				
					$("#" + box+" tr:eq(0) ."+ col +"").attr("class" , $("#" + box+" tr:eq(0) ."+ col +"").attr("class").replace('normal','current'));
				
					
				}
