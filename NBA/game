			}
			var currentcol = ['gs','gs'];
			var first = [0,0];
			var colorder = [1,1];
			function rank_table(box, col,tm)
			{
				var bt = tm;
				if(tm == '0')
					tm = 'CLE';
				else
					tm = 'TOR';
				box = box + tm;
				
				if(col == currentcol[bt])
					colorder[bt] = 1 - colorder[bt];
				else
					colorder[bt] = 1;
				var length = $("#" + box+" .sort").length;
				var o = new Array();
				var v = new Array();
				for(var i = 0; i < length ; i++)
				{
					o[i] = i;
					v[i] = parseFloat($("#" + box+" .sort:eq("+i+") ."+ col +"").attr('rank'));
				}
				var html = '';
				for(var j = 0; j < length; j++){
					for(var i = j + 1; i < length; i++){
						if(colorder[bt] == 0)
						{
							if(v[o[i]] < v[o[j]])
							{
								var t = o[i];
								o[i] = o[j];
								o[j] = t;
							}
						} else{
							if(v[o[i]] > v[o[j]])
							{
								var t = o[i];
								o[i] = o[j];
								o[j] = t;
							}
						}
					}
					html += $("#" + box+" .sort:eq("+o[j]+")")[0].outerHTML;
				}
				$("#" + box+" .sort").remove();
				$("#" + box+" tr:eq("+(1) + ")").before(html);
				to_current(bt,col,box);
				change_color();
			}
