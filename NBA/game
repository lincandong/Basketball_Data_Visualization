<script type="text/javascript">
			function change_color(){
				$('#stat_boxCLE .change_color').hover(function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxCLE .' + cls[3]).css('background', '#011233')
					$('#stat_boxCLE .' + cls[4]).css('background', '#011233')
				}, function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxCLE .' + cls[3]).css('background', 'none')
					$('#stat_boxCLE .' + cls[4]).css('background', 'none')
				})
				$('#stat_boxCLE .change_col').hover(function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxCLE .' + 'col' + cls[3]).css('background', '#011233')
				}, function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxCLE .' + 'col' + cls[3]).css('background', 'none')
				})
				$('#stat_boxTOR .change_color').hover(function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxTOR .' + cls[3]).css('background', '#011233')
					$('#stat_boxTOR .' + cls[4]).css('background', '#011233')
				}, function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxTOR .' + cls[3]).css('background', 'none')
					$('#stat_boxTOR .' + cls[4]).css('background', 'none')
				})
				$('#stat_boxTOR .change_col').hover(function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxTOR .' + 'col' + cls[3]).css('background', '#011233')
				}, function(){
					var cls = $(this).attr("class").split(' ');
					$('#stat_boxTOR .' + 'col' + cls[3]).css('background', 'none')
				})
			}
			change_color();
			
			function to_current(bt, col,box){
				var length = $("#" + box+" .sort").length;
				if(col == currentcol[bt]){
					var tt = $("#" + box+" tr:eq(0) ."+ col +"").html();
					$("#" + box+" tr:eq(0) ."+ col +"").html(tt.split(/<IMG/)[0].split(/<img/)[0]);
					$("#" + box+" tr:eq(0) ."+ col +"").html($("#" + box+" tr:eq(0) ."+ col +"").html() + '<img class="arrow" src="/css/stat_box/order_arrow'+colorder[bt]+'.png"/>');
				}
				else
				{
					$("#" + box+" tr:eq(0) ."+ col +"").html($("#" + box+" tr:eq(0) ."+ col +"").html() + '<img class="arrow" src="/css/stat_box/order_arrow1.png"/>');
				
					for(var i = 0; i < length; i++){
						$("#" + box+" .sort:eq("+i+") ."+ 'current' +"").attr("class" , $("#" + box+" .sort:eq("+i+") ."+ 'current' +"").attr("class").replace('current','normal'));
						$("#" + box+" .sort:eq("+i+") ."+ col +"").attr("class" , $("#" + box+" .sort:eq("+i+") ."+ col +"").attr("class").replace('normal','current'));
					}
